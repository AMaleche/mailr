<ul class="contacts">
  <% for contact in @contacts do -%>
    <li class="contact">
    <%=h contact.fname %> <%= h contact.lname %> 
    <div class='email'>
      <div class="notviscode">&lt;</div>
      <%= h contact.email %>
      <div class="notviscode">&gt;</div>
    </div>
    </li>
  <% end -%>
</ul>
